{
  "compilerOptions": {
    "moduleResolution": "node",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "allowJs": true,
    "baseUrl": ".",
    "typeRoots": [
      "node_modules/@types"
    ],
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@public":[
        "public/*"
      ],
      "@models":[
        "src/models/*"
      ],
      "@hooks":[
        "src/hooks/*"
      ],
      "@store":[
        "src/store/*"
      ],
    },
  },
  "include": [
    "./ext.d.ts"
  ],
  "files": [
    "src/index.tsx"
  ]
}
